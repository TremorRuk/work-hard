from math import*
x = 2
z = 1
t = 1
r1 = x**((2*z)**t)+((x**(2*z))**t)+x**t**(2*z)
print("r1 =", r1)
x2 = 4
r2 = (2*(sin((2*x2)**2))**5)+(e**(3*cos(x2)))-7
print("r2 =", r2)
x3 = 2
y3 = 2
z3 = 1
t3 = 1
r3 = ((5*x3*y3*z3)/(4*3*2))+(abs(x3))**(3*t3)
print("r3 =", r3)
x4 = 2
y4 = 2
r4 = (abs(log((x4**4)+3))-tan(x4/2))/(3*x4)-(4*y4)
print("r4 =", r4)
