N = 10
X = []
for i in range(0,5):
    x = float(input("Введите x: "))
    # print("type_x:", type(x))
    X.append(x)

#X=[0.1,0.3, 0.4, 0.7, 1]
print(' -------------------------')
print(' | значение         |         сумма |')
print(' -------------------------')

for x in X:
    s = 0
    u = (-1*x)/(2*1-1)
    for k in range(1,N+1):
        s += u/k
    print(" | %8.4f | %8.4f |" % (x,s))

print(' -------------------------')
